{
  "name": "teamaster-io",
  "version": "1.0.0",
  "description": "multi-page app that gathers customer feedback with email surveys",
  "main": "/client/src/index.js",
  "engines": {
    "node": ">= 12.6.0",
    "npm": ">= 6.10.0"
  },
  "proxy": {
    "/api/*": {
      "target": "http://localhost:5000"
    }
  },
  "scripts": {
    "start:dev": "nodemon server/index.js",
    "build:dev": "webpack -d --watch --mode development --env.ENVIRONMENT=development",
    "webhook": "ssh -R teamaster-io:80:localhost:5000 serveo.net",
    "dev": "concurrently \"npm run start:dev\" \"npm run build:dev\" \"npm run webhook\"",
    "start": "node server/index.js",
    "heroku-postbuild": "webpack -p --env.ENVIRONMENT=production"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cookie-session": "^1.3.3",
    "core-js": "^3.1.4",
    "express": "^4.17.1",
    "lodash": "^4.17.11",
    "materialize-css": "^1.0.0",
    "mongoose": "^5.6.4",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "path-parser": "^4.2.0",
    "querystringify": "^2.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.5",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-stripe-checkout": "^2.6.3",
    "redux": "^4.0.2",
    "redux-form": "^8.2.4",
    "redux-thunk": "^2.3.0",
    "sendgrid": "^5.2.3",
    "stripe": "^7.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
